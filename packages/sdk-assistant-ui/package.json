{
  "name": "@legit-sdk/assistant-ui",
  "version": "0.2.5",
  "description": "Legit integration for Assistant UI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "build": "tsc && rollup -c rollup.dts.config.js && node esbuild.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "PolyForm Noncommercial License 1.0.0",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/server.js",
    "dist/server.d.ts"
  ],
  "peerDependencies": {
    "react": ">=18",
    "assistant-cloud": "0.x",
    "assistant-stream": "0.x",
    "@assistant-ui/react": "0.x"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "esbuild": "^0.24.0",
    "happy-dom": "^14.12.3",
    "memfs": "^4.14.0",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.9.3",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@legit-sdk/core": "workspace:*",
    "ai": "^5.0.59"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.4.1"
}
